<%- include("partials/header") -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input type="text" class="form-control" name="postTitle" value="" autocomplete="off">
  </div>


  <div class="form-group">
    <label for="postContent">Post</label>
    <textarea class="form-control" name="postContent" rows="10"></textarea>
  </div>
  <button class="btn publish-btn" type="submit" name="button">Publish</button>

</form>


<%- include("partials/footer") -%>
